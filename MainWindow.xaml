<Window x:Class="Chat.gpt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Chat.gpt"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name ="isEnable" Content="IsEnabel" HorizontalAlignment="Left" Margin="202,199,0,0" VerticalAlignment="Top"/>
        <Button Content="Спросить" HorizontalAlignment="Left" Margin="383,298,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="27" Width="101" Click="Button_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.397"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <TextBox x:Name="input" HorizontalAlignment="Left" Margin="166,299,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="207" RenderTransformOrigin="-0.325,0.114" Height="26" TextChanged="TextBox_TextChanged"/>
       
    </Grid>
</Window>
